{
  "name": "improviz",
  "displayName": "Improviz",
  "description": "Allows interacting with Improviz by sending it code to run",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:improviz.runInImproviz",
    "onCommand:improviz.toggleText"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "improviz.runInImproviz",
      "title": "Run in Improviz"
    },{
      "command": "improviz.toggleText",
      "title": "Toggle Improviz Text"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
